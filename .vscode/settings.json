  "files.insertFinalNewline": true,
  "cSpell.enableFiletypes": [
    "typescriptreact"
  ],
  "cSpell.language": "en-GB,es",
  "eslint.workingDirectories": [
    ".",
    "/packages/*",
    "/apps/*"
  ],
