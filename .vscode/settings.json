    [ "cnm([^รง]*?(base\\:|dynamicVariants\\:)+[^รง]*?)\\}\\);?(\r\n|\r|\n)", "[\"'`]([^\"'`]*).*?[\"'`]" ],
    [ "classNameManager([^รง]*?(base\\:|dynamicVariants\\:)+[^รง]*?)\\}\\);?(\r\n|\r|\n)", "[\"'`]([^\"'`]*).*?[\"'`]" ]
  ]
}
